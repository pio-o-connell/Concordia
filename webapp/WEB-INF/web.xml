<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
                             https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">

    <display-name>Concordia App</display-name>

    <servlet>
        <servlet-name>ConcordiaServlet</servlet-name>
        <servlet-class>Concordia.controller.ConcordiaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConcordiaServlet</servlet-name>
        <url-pattern>/api/concordia</url-pattern>
    </servlet-mapping>


</web-app>
